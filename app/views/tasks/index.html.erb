<h1> My Tasks </h1>

  <% @tasks.each do |task| %>

    <% if task.completed == true%>
    <h4>[X] <%= link_to task.title, task_path(task) %> <%= link_to "Delete", delete_path(@task)  %>
       <h4>[&nbsp;  ] <%= link_to task.title, task_path(task) %>  <%= link_to "Delete", delete_path(@task)  %>
  <% end; end %>

<%= link_to "Create a new task", new_path %>

